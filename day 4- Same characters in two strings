class Solution 
{ 
    int sameChar(String a, String b) 
    {
      a =  a.toUpperCase();
       b = b.toUpperCase();
        int i=0;
        int n = a.length();
        int count =0;
        while(i<n){
            char first= a.charAt(i);
            char second = b.charAt(i);
            if(first==second){
                count++;
            }
            i++;
        }
        return count;
    }
} 
